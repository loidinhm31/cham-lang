{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:tray:default",
    "opener:default",
    "google-auth:default",
    "dialog:default",
    "dialog:allow-save",
    "dialog:allow-open",
    "dialog:allow-message",
    "notification:default",
    "schedule-task:default",
    "schedule-task:allow-schedule-task"
  ]
}
